<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9" xmlns:image="http://www.google.com/schemas/sitemap-image/1.1" >

    {% for url in urls %}
        {% if url.loc is defined and url.loc and url.loc != '' %}
            <url>
                <loc>{{ url.loc }}</loc>
                {% if url.lastmod is defined and url.lastmod %}
                    <lastmod>{{ url.lastmod }}</lastmod>
                {% endif %}
                {% if url.changefreq is defined and url.changefreq %}
                    <changefreq>{{ url.changefreq }}</changefreq>
                {% endif %}
                {% if url.priority is defined and url.priority %}
                    <priority>{{ url.priority }}</priority>
                {% endif %}
                {% if url.image is defined and url.image %}
                    <image:image><image:loc><![CDATA[{{ url.image }}]]></image:loc></image:image>
                {% endif %}
            </url>
        {% endif %}
    {% endfor %}
</urlset>
