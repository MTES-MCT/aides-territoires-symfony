<nav id="account-sidebar-menu" class="fr-col fr-col-md-3 fr-sidemenu fr-mb-2w" role="navigation" aria-label="Menu latéral">
    <div class="fr-sidemenu__inner">
        <button class="fr-sidemenu__btn" aria-controls="fr-sidemenu-wrapper" aria-expanded="false">Mon compte</button>
        <div class="fr-collapse" id="fr-sidemenu-wrapper">
            <ul class="fr-sidemenu__list">
                {% if hideMenu is not defined %}
                <li class="fr-sidemenu__item {% if dashboard is defined %}fr-sidemenu__item--active{% endif %}">
                    <a class="fr-sidemenu__link" href="{{ path('app_user_dashboard') }}" target="_self" {% if dashboard is defined %}aria-current="page"{% endif %} rel="noopener">Tableau de bord</a>
                </li>     
                {% if app.user.isContributor %}
                <li class="fr-sidemenu__item">
                    <button class="fr-sidemenu__btn" {% if publishAid is defined %} aria-expanded="true" {% else %} aria-expanded="false" {% endif %} aria-controls="fr-sidemenu-item-2">Diffuser mes aides</button>
                    <div class="fr-collapse" id="fr-sidemenu-item-2">
                        <ul class="fr-sidemenu__list">
                            <li class="fr-sidemenu__item {% if aidDraftList is defined %}fr-sidemenu__item--active{% endif %}">
                                <a class="fr-sidemenu__link" href="{{ path('app_user_aid_publications') }}" target="_self"  {% if aidDraftList is defined %}aria-current="page"{% endif %} rel="noopener">Mon portefeuille d’aides</a>
                            </li>
                            <li class="fr-sidemenu__item">
                                <a class="fr-sidemenu__link" href="{{ path('app_user_aid_detail_publish') }}" target="_self" rel="noopener">Publier une aide</a>
                            </li>
                        </ul>
                    </div>
                </li>
                {% endif %}

                <li class="fr-sidemenu__item">
                    <button class="fr-sidemenu__btn" {% if user_project is defined %} aria-expanded="true" {% else %} aria-expanded="false" {% endif %} aria-controls="fr-sidemenu-item-3">Mes projets</button>
                    <div class="fr-collapse" id="fr-sidemenu-item-3">
                        <ul class="fr-sidemenu__list">
                            <li class="fr-sidemenu__item {% if user_project_structure is defined %}fr-sidemenu__item--active{% endif %}">
                                <a class="fr-sidemenu__link" href="{{ path('app_user_project_structure') }}" target="_self" {% if user_project_structure is defined %}aria-current="page"{% endif %} rel="noopener">
                                    De ma structure
                                </a>
                            </li>
                            <li class="fr-sidemenu__item {% if user_project_favoris is defined %}fr-sidemenu__item--active{% endif %}">
                                <a class="fr-sidemenu__link" href="{{ path('app_user_project_favoris') }}" target="_self" {% if user_project_favoris is defined %}aria-current="page"{% endif %} rel="noopener">
                                    Favoris
                                </a>
                            </li>                           
                        </ul>
                    </div>
                </li>

                <li class="fr-sidemenu__item {% if alert_list is defined %}fr-sidemenu__item--active{% endif %}">
                    <a class="fr-sidemenu__link" href="{{ path('app_user_alert') }}" target="_self" rel="noopener" {% if alert_list is defined %}aria-current="page"{% endif %} title="Gérer mes alertes">Gérer mes alertes</a>
                </li>

                <li class="fr-sidemenu__item">
                    <button class="fr-sidemenu__btn"  {% if structure is defined %} aria-expanded="true" {% else %} aria-expanded="false" {% endif %} aria-controls="fr-sidemenu-item-4">Ma structure</button>
                    <div class="fr-collapse" id="fr-sidemenu-item-4">
                        <ul class="fr-sidemenu__list">
                            
                            <li class="fr-sidemenu__item {% if structure_info is defined %}fr-sidemenu__item--active{% endif %}">
                                <a class="fr-sidemenu__link" href="{{ path('app_organization_structure_information') }}" target="_self" {% if structure_info is defined %}aria-current="page"{% endif %} rel="noopener">Informations</a>
                            </li>
                            {% if app.user.defaultOrganization %}
                                {% if app.user.defaultOrganization.organizationType.slug in ["commune", "epci", "department", "region"] %}
                                <li class="fr-sidemenu__item {% if data_info is defined %}fr-sidemenu__item--active{% endif %}">
                                    <a class="fr-sidemenu__link" href="{{ path('app_organization_donnees_cles') }}" target="_self" {% if data_info is defined %}aria-current="page"{% endif %} rel="noopener">Données clés</a>
                                </li>
                                {% endif %}
                            {% endif %}
                            <li class="fr-sidemenu__item {% if structure_collab is defined %}fr-sidemenu__item--active{% endif %}">
                                <a class="fr-sidemenu__link" href="{{ path('app_organization_collaborateurs') }}" target="_self" {% if structure_collab is defined %}aria-current="page"{% endif %} rel="noopener">Collaborateurs</a>
                            </li>
                            <li class="fr-sidemenu__item {% if structure_invitations is defined %}fr-sidemenu__item--active{% endif %}">
                                <a class="fr-sidemenu__link" href="{{ path('app_organization_invitations') }}" target="_self" {% if structure_invitations is defined %}aria-current="page"{% endif %} rel="noopener">Mes invitations</a>
                            </li>
                        </ul>
                    </div>
                </li>

                <li class="fr-sidemenu__item">
                    <button class="fr-sidemenu__btn" {% if user_parameter is defined %} aria-expanded="true" {% else %} aria-expanded="false" {% endif %} aria-controls="fr-sidemenu-item-5">Mes paramètres</button>
                    <div class="fr-collapse" id="fr-sidemenu-item-5">
                        <ul class="fr-sidemenu__list">
                            <li class="fr-sidemenu__item {% if user_profile is defined %}fr-sidemenu__item--active{% endif %}">
                                <a class="fr-sidemenu__link" href="{{ path('app_user_parameter_profil') }}" target="_self" {% if user_profile is defined %}aria-current="page"{% endif %} rel="noopener">
                                    Mon profil
                                </a>
                            </li>
                            <li class="fr-sidemenu__item {% if api_token is defined %}fr-sidemenu__item--active{% endif %}">
                                <a class="fr-sidemenu__link" href="{{ path('app_user_parameter_api_token') }}" target="_self" {% if api_token is defined %}aria-current="page"{% endif %} rel="noopener">
                                    Ma clé API
                                </a>
                            </li>
                            <li class="fr-sidemenu__item {% if history_login is defined %}fr-sidemenu__item--active{% endif %}">
                                <a class="fr-sidemenu__link" href="{{ path('app_user_parameter_history_log') }}" target="_self" {% if history_login is defined %}aria-current="page"{% endif %} rel="noopener">
                                    Mon journal de connexion
                                </a>
                            </li>
                        </ul>
                    </div>
                </li>         
                {# <li class="fr-sidemenu__item fr-py-2w">
                    <div class="at-sidemenu__separator fr-px-2w"><hr/></div>
                </li>                 #}
                <li class="fr-sidemenu__item fr-mt-2w">
                    <div class="fr-pr-2w fr-pl-2w"><hr class="fr-hint-text"/></div>
                </li>
                {% endif %}
                <li class="fr-sidemenu__item">
                    <a class="fr-sidemenu__link" href="{{ path('app_logout') }}" target="_self" rel="noopener">Se déconnecter</a>
                </li>
                
            </ul>
        </div>
    </div>
</nav>